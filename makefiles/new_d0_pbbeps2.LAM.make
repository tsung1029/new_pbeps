globals.o	$(FC) $(OPTS90) -c pbfield2mod.fpnpfield2mod.o : pnpfield2mod.f pfield2mod.o pdfield2mod.o pbfield2mod.o	$(FC) $(OPTS90) -c pnpfield2mod.fpdiag2mod.o : pdiag2mod.f pinit2mod.o	$(FC) $(OPTS90) -c pdiag2mod.fp0mod.o : p0mod.f globals.o	$(FC) $(OPTS90) -c p0mod.fp2mod.o : p2mod.f p0mod.o	$(FC) $(OPTS90) -c p2mod.fnew_d0_pbbeps2.o : new_d0_pbbeps2.f prbpush2mod.o pbpush2mod.o ppush2mod.o \                   pnpfield2mod.o pdiag2mod.o p2mod.o	$(FC) $(OPTS90) -c new_d0_pbbeps2.fclean :# Delete -i new_d0_pbbeps2.out $(OBJS)