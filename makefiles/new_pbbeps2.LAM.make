# Makefile for new_pbbeps2 with MacOS X using IBM compilerFC = xlf90OPTS90 = -O3 -qautodbl=dbl4 -qarch=g5OBJS = new_pbbeps2.o \globals.o pinit2mod.o prbpush2mod.o pbpush2mod.o ppush2mod.o \pfield2mod.o pdiag2mod.o p2mod.o p0mod.o pinit2lib.o prbpush2lib.o \pbpush2lib.o ppush2lib.o pfield2lib.o pdiag2lib.o p2lib.o p0lib.o \nullpgks2.o nullpgks1.o nullLOG.o # Linkage rulenew_pbbeps2.out : $(OBJS)	mpif77  $(OPTS90) -o new_pbbeps2.out $(OBJS) \        /System/Library/Frameworks/Carbon.framework/Carbon# Compilation rulesnullLOG.o : nullLOG.f	$(FC) $(OPTS90) -qfixed -c nullLOG.fnullpgks1.o : nullpgks1.f	$(FC) $(OPTS90) -qfixed -c nullpgks1.fnullpgks2.o : nullpgks2.f	$(FC) $(OPTS90) -qfixed -c nullpgks2.fp0lib.o : p0lib.f	mpif77  $(OPTS90) -qfixed -c p0lib.fp2lib.o : p2lib.f	$(FC) $(OPTS90) -qfixed -c p2lib.fpinit2lib.o : pinit2lib.f	$(FC) $(OPTS90) -qfixed -c pinit2lib.fprbpush2lib.o : prbpush2lib.f	$(FC) $(OPTS90) -qfixed -c prbpush2lib.fpbpush2lib.o : pbpush2lib.f	$(FC) $(OPTS90) -qfixed -c pbpush2lib.fppush2lib.o : ppush2lib.f	$(FC) $(OPTS90) -qfixed -c ppush2lib.fpfield2lib.o : pfield2lib.f	$(FC) $(OPTS90) -qfixed -c pfield2lib.fpdiag2lib.o : pdiag2lib.f	$(FC) $(OPTS90) -qfixed -c pdiag2lib.fglobals.o : globals.f	$(FC) $(OPTS90) -c globals.fpinit2mod.o : pinit2mod.f globals.o	$(FC) $(OPTS90) -c pinit2mod.fppush2mod.o : ppush2mod.f p0mod.o	$(FC) $(OPTS90) -c ppush2mod.fpbpush2mod.o : pbpush2mod.f p0mod.o	$(FC) $(OPTS90) -c pbpush2mod.fprbpush2mod.o : prbpush2mod.f p0mod.o	$(FC) $(OPTS90) -c prbpush2mod.fpfield2mod.o : pfield2mod.f globals.o	$(FC) $(OPTS90) -c pfield2mod.fpdiag2mod.o : pdiag2mod.f pinit2mod.o	$(FC) $(OPTS90) -c pdiag2mod.fp0mod.o : p0mod.f globals.o	$(FC) $(OPTS90) -c p0mod.fp2mod.o : p2mod.f p0mod.o	$(FC) $(OPTS90) -c p2mod.fnew_pbbeps2.o : new_pbbeps2.f prbpush2mod.o pbpush2mod.o ppush2mod.o \                pfield2mod.o pdiag2mod.o p2mod.o	$(FC) $(OPTS90) -c new_pbbeps2.fclean :# Delete -i new_pbbeps2.out $(OBJS)