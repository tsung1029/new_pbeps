# Makefile for new_mpbbeps2 with MacOS X using IBM compilerFC = xlf90OPTS90 = -O3 -qautodbl=dbl4 -qarch=g5OBJS = new_mpbbeps2.o \globals.o pinit2mod.o mprbpush2mod.o mpbpush2mod.o mppush2mod.o prbpush2mod.o \pbpush2mod.o ppush2mod.o mpfield2mod.o pfield2mod.o pdiag2mod.o p2mod.o \p0mod.o pinit2lib.o mprbpush2lib.o mpbpush2lib.o mppush2lib.o prbpush2lib.o \pbpush2lib.o ppush2lib.o mpfield2lib.o pfield2lib.o pdiag2lib.o p2lib.o \p0lib.o MacMPxlf.o MacMP.o nullpgks2.o nullpgks1.o nullLOG.o # Linkage rulenew_mpbbeps2.out : $(OBJS)	mpif77  $(OPTS90) -o new_mpbbeps2.out $(OBJS) \        /System/Library/Frameworks/Carbon.framework/Carbon# Compilation rulesnullLOG.o : nullLOG.f	$(FC) $(OPTS90) -qfixed -c nullLOG.fMacMPxlf.o : MacMPxlf.c	gcc -O -c MacMPxlf.cMacMP.o : MacMP.c	gcc -O -c -I /Developer/Headers/FlatCarbon MacMP.cnullpgks1.o : nullpgks1.f	$(FC) $(OPTS90) -qfixed -c nullpgks1.fnullpgks2.o : nullpgks2.f	$(FC) $(OPTS90) -qfixed -c nullpgks2.fp0lib.o : p0lib.f	mpif77  $(OPTS90) -qfixed -c p0lib.fp2lib.o : p2lib.f	$(FC) $(OPTS90) -qfixed -c p2lib.fpinit2lib.o : pinit2lib.f	$(FC) $(OPTS90) -qfixed -c pinit2lib.fmprbpush2lib.o : mprbpush2lib.f	$(FC) $(OPTS90) -qfixed -c mprbpush2lib.fmpbpush2lib.o : mpbpush2lib.f	$(FC) $(OPTS90) -qfixed -c mpbpush2lib.fmppush2lib.o : mppush2lib.f	$(FC) $(OPTS90) -qfixed -c mppush2lib.fprbpush2lib.o : prbpush2lib.f	$(FC) $(OPTS90) -qfixed -c prbpush2lib.fpbpush2lib.o : pbpush2lib.f	$(FC) $(OPTS90) -qfixed -c pbpush2lib.fppush2lib.o : ppush2lib.f	$(FC) $(OPTS90) -qfixed -c ppush2lib.fmpfield2lib.o : mpfield2lib.f	$(FC) $(OPTS90) -qfixed -c mpfield2lib.fpfield2lib.o : pfield2lib.f	$(FC) $(OPTS90) -qfixed -c pfield2lib.fpdiag2lib.o : pdiag2lib.f	$(FC) $(OPTS90) -qfixed -c pdiag2lib.fglobals.o : globals.f	$(FC) $(OPTS90) -c globals.fpinit2mod.o : pinit2mod.f globals.o	$(FC) $(OPTS90) -c pinit2mod.fppush2mod.o : ppush2mod.f p0mod.o	$(FC) $(OPTS90) -c ppush2mod.fpbpush2mod.o : pbpush2mod.f p0mod.o	$(FC) $(OPTS90) -c pbpush2mod.fprbpush2mod.o : prbpush2mod.f p0mod.o	$(FC) $(OPTS90) -c prbpush2mod.fmppush2mod.o : mppush2mod.f ppush2mod.o	$(FC) $(OPTS90) -c mppush2mod.fmpbpush2mod.o : mpbpush2mod.f pbpush2mod.o	$(FC) $(OPTS90) -c mpbpush2mod.fmprbpush2mod.o : mprbpush2mod.f prbpush2mod.o	$(FC) $(OPTS90) -c mprbpush2mod.fpfield2mod.o : pfield2mod.f globals.o	$(FC) $(OPTS90) -c pfield2mod.fmpfield2mod.o : mpfield2mod.f pfield2mod.o	$(FC) $(OPTS90) -c mpfield2mod.fpdiag2mod.o : pdiag2mod.f pinit2mod.o	$(FC) $(OPTS90) -c pdiag2mod.fp0mod.o : p0mod.f globals.o	$(FC) $(OPTS90) -c p0mod.fp2mod.o : p2mod.f p0mod.o	$(FC) $(OPTS90) -c p2mod.fnew_mpbbeps2.o : new_mpbbeps2.f mprbpush2mod.o mpbpush2mod.o mppush2mod.o \                 mpfield2mod.o pdiag2mod.o p2mod.o	$(FC) $(OPTS90) -c new_mpbbeps2.fclean :# Delete -i new_mpbbeps2.out $(OBJS)